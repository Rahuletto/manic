{
  "name": "@manicjs/providers",
  "version": "0.1.0",
  "description": "Deployment providers for Manic framework - Vercel, Cloudflare, Netlify",
  "type": "module",
  "license": "GPL-3.0",
  "author": "Rahuletto",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rahuletto/manic"
  },
  "keywords": [
    "manic",
    "vercel",
    "cloudflare",
    "netlify",
    "deploy",
    "adapter"
  ],
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./vercel": {
      "types": "./src/vercel/index.ts",
      "import": "./src/vercel/index.ts"
    },
    "./cloudflare": {
      "types": "./src/cloudflare/index.ts",
      "import": "./src/cloudflare/index.ts"
    },
    "./netlify": {
      "types": "./src/netlify/index.ts",
      "import": "./src/netlify/index.ts"
    }
  },
  "files": [
    "src/**/*",
    "README.md"
  ],
  "peerDependencies": {
    "manicjs": ">=0.4.0"
  },
  "dependencies": {
    "colorette": "^2.0.20"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  }
}
